{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.5.2",
    "axios": "^0.19.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "5.0.1",
    "styled-components": "^4.4.1"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development react-app-rewired start",
    "build": "cross-env NODE_ENV=production GENERATE_SOURCEMAP=false react-app-rewired build",
    "test": "echo \"No tests\" && exit 0",
    "eject": "echo \"Eject is not allowed\" && exit 1"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "sort-destructure-keys",
      "sort-exports"
    ],
    "rules": {
      "sort-exports/sort-exports": [
        "error",
        {
          "sortDir": "asc",
          "ignoreCase": false
        }
      ],
      "sort-destructure-keys/sort-destructure-keys": [
        "error",
        {
          "caseSensitive": true
        }
      ],
      "sort-keys": "error",
      "import/group-exports": "error",
      "react/jsx-sort-props": [
        "error",
        {
          "callbacksLast": true,
          "shorthandFirst": true,
          "shorthandLast": false,
          "ignoreCase": false,
          "noSortAlphabetically": false,
          "reservedFirst": true
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "^6.0.3",
    "eslint-plugin-sort-destructure-keys": "^1.3.3",
    "eslint-plugin-sort-exports": "^0.2.0",
    "react-app-rewired": "^2.1.4"
  }
}
